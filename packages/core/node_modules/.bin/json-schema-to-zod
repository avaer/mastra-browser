#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/a/mastra/node_modules/.pnpm/json-schema-to-zod@2.6.0/node_modules/json-schema-to-zod/dist/cjs/node_modules:/Users/a/mastra/node_modules/.pnpm/json-schema-to-zod@2.6.0/node_modules/json-schema-to-zod/dist/node_modules:/Users/a/mastra/node_modules/.pnpm/json-schema-to-zod@2.6.0/node_modules/json-schema-to-zod/node_modules:/Users/a/mastra/node_modules/.pnpm/json-schema-to-zod@2.6.0/node_modules:/Users/a/mastra/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/a/mastra/node_modules/.pnpm/json-schema-to-zod@2.6.0/node_modules/json-schema-to-zod/dist/cjs/node_modules:/Users/a/mastra/node_modules/.pnpm/json-schema-to-zod@2.6.0/node_modules/json-schema-to-zod/dist/node_modules:/Users/a/mastra/node_modules/.pnpm/json-schema-to-zod@2.6.0/node_modules/json-schema-to-zod/node_modules:/Users/a/mastra/node_modules/.pnpm/json-schema-to-zod@2.6.0/node_modules:/Users/a/mastra/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../json-schema-to-zod/dist/cjs/cli.js" "$@"
else
  exec node  "$basedir/../json-schema-to-zod/dist/cjs/cli.js" "$@"
fi
