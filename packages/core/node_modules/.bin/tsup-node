#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/a/mastra/node_modules/.pnpm/tsup@8.4.0_@microsoft+api-extractor@7.52.2_@types+node@20.17.27__@swc+core@1.11.10_@swc_652f4f4d3e7dfb74c227158e300e9a7f/node_modules/tsup/dist/node_modules:/Users/a/mastra/node_modules/.pnpm/tsup@8.4.0_@microsoft+api-extractor@7.52.2_@types+node@20.17.27__@swc+core@1.11.10_@swc_652f4f4d3e7dfb74c227158e300e9a7f/node_modules/tsup/node_modules:/Users/a/mastra/node_modules/.pnpm/tsup@8.4.0_@microsoft+api-extractor@7.52.2_@types+node@20.17.27__@swc+core@1.11.10_@swc_652f4f4d3e7dfb74c227158e300e9a7f/node_modules:/Users/a/mastra/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/a/mastra/node_modules/.pnpm/tsup@8.4.0_@microsoft+api-extractor@7.52.2_@types+node@20.17.27__@swc+core@1.11.10_@swc_652f4f4d3e7dfb74c227158e300e9a7f/node_modules/tsup/dist/node_modules:/Users/a/mastra/node_modules/.pnpm/tsup@8.4.0_@microsoft+api-extractor@7.52.2_@types+node@20.17.27__@swc+core@1.11.10_@swc_652f4f4d3e7dfb74c227158e300e9a7f/node_modules/tsup/node_modules:/Users/a/mastra/node_modules/.pnpm/tsup@8.4.0_@microsoft+api-extractor@7.52.2_@types+node@20.17.27__@swc+core@1.11.10_@swc_652f4f4d3e7dfb74c227158e300e9a7f/node_modules:/Users/a/mastra/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
