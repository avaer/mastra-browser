{
  "name": "@mastra/core",
  "version": "0.0.15",
  "license": "ISC",
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./*": {
      "import": "./src/*/index.ts",
      "require": "./src/*/index.ts"
    },
    "./deployer": {
      "import": "./src/deployer/index.ts",
      "require": "./src/deployer/index.ts"
    },
    "./storage/libsql": {
      "import": "./src/storage/libsql/index.ts",
      "require": "./src/storage/libsql/index.ts"
    },
    "./storage/pglite": {
      "import": "./src/storage/pglite/index.ts",
      "require": "./src/storage/pglite/index.ts"
    },
    "./vector/libsql": {
      "import": "./src/vector/libsql/index.ts",
      "require": "./src/vector/libsql/index.ts"
    },
    "./vector/pglite": {
      "import": "./src/vector/pglite/index.ts",
      "require": "./src/vector/pglite/index.ts"
    },
    "./vector/filter": {
      "import": "./src/vector/filter/index.ts",
      "require": "./src/vector/filter/index.ts"
    },
    "./utils": {
      "import": "./src/utils.ts",
      "require": "./src/utils.ts"
    },
    "./base": {
      "import": "./src/base.ts",
      "require": "./src/base.ts"
    },
    "./telemetry/otel-vendor": {
      "import": "./src/telemetry/otel-vendor.ts",
      "require": "./src/telemetry/otel-vendor.ts"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "check": "tsc --noEmit",
    "analyze": "size-limit --why",
    "lint": "eslint .",
    "build": "tsup --config tsup.config.ts --format esm,cjs",
    "build:watch": "pnpm build --watch",
    "size": "size-limit",
    "test": "vitest run"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.2.17",
    "@libsql/client": "^0.14.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.56.1",
    "@opentelemetry/core": "^1.30.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.57.2",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.2",
    "@opentelemetry/otlp-exporter-base": "^0.57.2",
    "@opentelemetry/otlp-transformer": "^0.57.2",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-metrics": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.57.2",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "@opentelemetry/semantic-conventions": "^1.30.0",
    "ai": "^4.2.2",
    "cohere-ai": "^7.16.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.7",
    "json-schema": "^0.4.0",
    "json-schema-to-zod": "^2.6.0",
    "hono": "^4.5.1",
    "hono-openapi": "^0.4.6",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "radash": "^12.1.0",
    "sift": "^17.1.3",
    "xstate": "^5.19.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^1.2.1",
    "@babel/core": "^7.26.10",
    "@microsoft/api-extractor": "^7.52.1",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/babel__core": "^7.20.5",
    "@types/json-schema": "^7.0.15",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.27",
    "@types/pino": "^7.0.5",
    "@types/qs": "^6.9.18",
    "eslint": "^9.23.0",
    "rollup": "^4.35.0",
    "size-limit": "^11.2.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "ai",
    "llm",
    "llms",
    "agent",
    "agents",
    "vectorstore",
    "embeddings",
    "rag",
    "evals",
    "memory",
    "tools",
    "telemetry",
    "typescript",
    "opentelemetry",
    "otel",
    "framework"
  ]
}
